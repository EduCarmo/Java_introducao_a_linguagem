package operadoresDeIncrementoEDecrementoEIngualdade;

public class Operadores_De_Incre_Decre_igual {

	public static void main(String[] args) {
		int numero = 1;
		System.out.println("numero");
		
		//numero++; numer = numero + 1
		
		System.out.println(numero++);
		System.out.println(numero--);
		

	}

}
